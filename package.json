{
  "name": "vue-todo",
  "version": "0.1.0",
  "homepage": "https://github.com/irestone/vue-todo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irestone/vue-todo.git"
  },
  "bugs": {
    "url": "https://github.com/irestone/vue-todo/issues"
  },
  "scripts": {
    "start": "node -r dotenv/config -r esm ./bin/www",
    "dev": "run-p watch:*",
    "watch:server": "nodemon",
    "watch:views": "npm run serve --prefix app/views",
    "build": "npm run build --prefix app/views",
    "lint": "vue-cli-service lint \"**/*.{js,vue}\"",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.1.2",
    "debug": "~2.6.9",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vuetify": "^2.1.0",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^1.1.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  }
}
